<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Prism.Xamarin.Forms.Templates</id>
    <version>$version$</version>
    <authors>Brian Lagunas,Dan Siegel</authors>
    <description>Official Prism Library Templates for Xamarin.Forms apps. This includes a basic Blank Prism.Forms template as well a Module Template for builind modular apps.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/PrismLibrary/Prism</projectUrl>
    <license type="file">LICENSE.txt</license>
    <icon>images\prism-logo.png</icon>
    <tags>xamarin.forms prism prism.forms templates</tags>
    <repository type="git" url="https://github.com/PrismLibrary/Prism.Templates.git" branch="master" commit="$commitId$" />
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
  </metadata>
  <files>
    <file src="../LICENSE.txt" target="" />
    <file src="../prism-logo.png" target="images\" />
    <file src="BlankApp/**" exclude="**\.vs\**\*;**\bin\**\*;**\obj\**\*;**\*.user;**\*.lock.json;**\.DS_Store" target="content/BlankApp" />
    <file src="Module.XF/**" exclude="**\.vs\**\*;**\bin\**\*;**\obj\**\*;**\*.user;**\*.lock.json;**\.DS_Store" target="content/Module.XF" />
  </files>
</package>